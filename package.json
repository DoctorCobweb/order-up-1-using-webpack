{
  "name": "order-up-1-using-webpack",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev:renderer": "cross-env START_HOT=yes node --trace-warnings -r @babel/register ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack.config.dev.renderer.js",
    "dev:main:mock": "cross-env NODE_ENV=developmentMock node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack.js --config ./webpack.config.dev.main.js --colors",
    "dev:main:printer": "cross-env NODE_ENV=developmentPrinter node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack.js --config ./webpack.config.dev.main.js --colors",
    "dev:electron": "cross-env ELECTRON_APP_MAIN_URL=http://localhost:8181/appMain/index.html ELECTRON_APP_ONE_URL=http://localhost:8181/appOne/index.html electron ./app/dev.main.js",
    "dev:mock": "yarn dev:main:mock && yarn dev:renderer",
    "dev:printer": "yarn dev:main:printer && yarn dev:renderer",
    "build:renderer": "cross-env NODE_ENV=production node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack.js --config ./webpack.config.prod.renderer.js --colors",
    "build:main": "cross-env NODE_ENV=production node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack.js --config ./webpack.config.prod.main.js --colors",
    "build:both": "concurrently \"yarn build:renderer\" \"yarn build:main\"",
    "build:electron": "electron-builder build",
    "build": "yarn build:both && electron-builder build",
    "postinstall": "./node_modules/.bin/electron-builder install-app-deps package.json"
  },
  "build": {
    "productName": "OrderUp",
    "appId": "au.com.andretrosky.orderup",
    "extraMetadata": {
      "main": "./prod.main.js"
    },
    "files": [
      "./**/*",
      "package.json",
      "node_modules/",
      "prod.main.js",
      "prod.main.js.map"
    ],
    "directories": {
      "output": "release",
      "buildResources": "."
    }
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.3.2",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.1.0",
    "electron": "^3.0.3",
    "electron-builder": "^20.28.4",
    "electron-rebuild": "^1.8.2",
    "electron-redux": "^1.3.1",
    "electron-reload": "^1.2.5",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-stringify-pretty-compact": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.4",
    "electron-log": "^2.2.17",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "normalizr": "^3.3.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-modal": "^3.6.1",
    "react-redux": "^5.0.7",
    "redux-thunk": "^2.3.0",
    "rethinkdb": "^2.3.3",
    "uuid": "^3.3.2",
    "wait-on": "^3.1.0"
  }
}
