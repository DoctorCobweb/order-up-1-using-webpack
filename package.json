{
  "name": "order-up-1-using-webpack",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev:renderer": "cross-env NODE_ENV=development START_HOT=yes node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack.config.dev.renderer.js",
    "dev:main:mock": "cross-env NODE_ENV=development MOCKING_ORDERS=yes node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack.js --config ./webpack.config.dev.main.js --colors",
    "dev:main:printer": "cross-env NODE_ENV=development MOCKING_ORDERS=no node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack.js --config ./webpack.config.dev.main.js --colors",
    "dev:electron": "cross-env NODE_ENV=development electron ./app/dev.main.js",
    "dev:mock": "yarn dev:main:mock && yarn dev:renderer",
    "dev:printer": "yarn dev:main:printer && yarn dev:renderer",
    "build:renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack.js --config ./webpack.config.prod.renderer.js --colors",
    "build:main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack.js --config ./webpack.config.prod.main.js --colors",
    "build:both": "concurrently \"yarn build:renderer\" \"yarn build:main\"",
    "build:electron": "electron-builder build",
    "build": "yarn build:both && electron-builder build",
    "postinstall": "./node_modules/.bin/electron-builder install-app-deps package.json"
  },
  "build": {
    "productName": "OrderUp",
    "appId": "au.com.andretrosky.orderup",
    "extraMetadata": {
      "main": "./main.prod.js"
    },
    "files": [
      "dist/",
      "renderer-process/appMain/index.html",
      "renderer-process/appOne/index.html",
      "renderer-process/appTwo/index.html",
      "renderer-process/appThree/index.html",
      "node_modules/",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "directories": {
      "output": "release",
      "buildResources": "resources",
      "app": "app"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.3.2",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.1.0",
    "electron": "^3.0.3",
    "electron-builder": "^20.28.4",
    "electron-rebuild": "^1.8.2",
    "electron-redux": "^1.3.1",
    "electron-reload": "^1.2.5",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-stringify-pretty-compact": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "electron-log": "^2.2.17",
    "express": "^4.16.4",
    "formik": "^1.3.2",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "normalizr": "^3.3.0",
    "react": "^16.5.2",
    "react-beautiful-dnd": "^10.0.0",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-modal": "^3.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-select": "2.1.1",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2",
    "wait-on": "^3.1.0",
    "yup": "^0.26.6"
  }
}
